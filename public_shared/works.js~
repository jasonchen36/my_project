self.onmessage=function(k){(function(c,b,d,e){var f,g,a,h;null==b&&(b={});null==d&&(d=function(){});null==e&&(e="GET");f=[];for(g in b)h=b[g],f.push(""+g+"="+h);b=f.join("&");a=new XMLHttpRequest;a.open(e,c,!1);a.setRequestHeader("Content-type","application/x-www-form-urlencoded");a.onreadystatechange=function(){if(4===a.readyState&&200===a.status)return d(a.responseText)};a.send(b);return a})("/../filters",k.data,function(c){self.postMessage(c)},"POST")};
